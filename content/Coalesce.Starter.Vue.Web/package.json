{
  "name": "Coalesce.Starter.Vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test",
    "lint": "vue-cli-service lint",
    "coalesce": "dotnet coalesce"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "coalesce-vue": "3.0.0-alpha.20200717.1",
    "coalesce-vue-vuetify": "3.0.0-alpha.20200717.1",
    "typeface-roboto": "0.0.75",
    "vue": "2.6.11",
    "vue-class-component": "7.2.5",
    "vue-property-decorator": "9.0.0",
    "vue-router": "3.3.4",
    "vuetify": "2.3.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@types/jest": "25.1.2",
    "@vue/cli-plugin-babel": "4.2.2",
    "@vue/cli-plugin-typescript": "4.2.2",
    "@vue/cli-plugin-unit-jest": "4.2.2",
    "@vue/cli-service": "4.2.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "aspnet-webpack": "^3.0.0",
    "babel-core": "^7.0.0-0",
    "sass": "1.26.10",
    "sass-loader": "9.0.2",
    "ts-jest": "25.1.0",
    "typescript": "3.9.7",
    "vue-template-compiler": "2.6.11",
    "webpack-hot-middleware": "2.25.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/(tests/unit/**/*.spec.(ts|tsx|js)|**/__tests__/*.(ts|tsx|js))"
    ]
  }
}
